{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
      "include": ["./src/README.md", "./src/"],
      "exclude": ["./src/conventions/", "./test/", "scratch/"],
      "includePattern": ".+\\.js(doc)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs/api",
    "encoding": "utf8",
    "recurse": true,
    "template": "./node_modules/minami"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false
    },
    "path": "ink-docstrap",
    "theme": "cerulean",
    "navType": "vertical",
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
  }
}
